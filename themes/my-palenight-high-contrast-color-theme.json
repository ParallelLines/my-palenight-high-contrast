	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#676E95"
			}
		},
		{
			"name": "Variables & Args",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"variable.parameter.function", // Function arguments
				"meta.function.parameter" // Covers more cases in function definitions
			],
			"settings": {
				"foreground": "#adb2cf"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.function" // Function keywords
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"punctuation.section.block",
				"punctuation.definition.block", // { }
				"punctuation.definition.parameters", // ( )
				"meta.brace.round", // Extra scope for ( )
				"meta.brace.curly", // Extra scope for { }
				"punctuation.definition.template-expression", // ${} in template strings
				"punctuation.separator.comma", // ,
				"punctuation.terminator.statement", // ;
				"punctuation.separator.key-value", // :
				"keyword.control.return",
				"keyword.control.flow",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.logical", // &&, ||, !
				"keyword.operator.assignment", // =, +=, -=, etc.
				"keyword.operator.increment", // Covers ++
				"keyword.operator.decrement", // Covers --
				"keyword.control.loop", // 'for', 'while', 'do'
				"keyword.control.conditional", // 'if', 'else', 'switch', 'case'
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Argument Braces & Conditional Braces",
			"scope": [
				"punctuation.definition.parameters",
				"punctuation.definition.condition", // ( ) in if, for, while
				"punctuation.section.block" // { } in if, for, while, functions
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#adb2cf"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#adb2cf"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
